{
  "name": "arnellebalane.com",
  "version": "1.0.0",
  "description": "my personal website",
  "main": "index.js",
  "scripts": {
    "clean:static": "rm -rf build",
    "clean:functions": "rm -rf build-functions",
    "clean": "npm-run-all --parallel clean:static clean:functions",
    "prebuild:static": "npm run clean:static",
    "build:static": "webpack",
    "prebuild:functions": "npm run clean:functions",
    "build:functions": "netlify-lambda build functions",
    "build": "npm-run-all --parallel build:static build:functions",
    "lint:js": "eslint \"{source,functions}/**/*.js\" \"*.js\"",
    "lint:css": "stylelint \"source/**/*.css\"",
    "lint": "npm-run-all --sequential lint:js lint:css",
    "serve": "webpack-dev-server --compress",
    "lighthouse": "lighthouse-ci https://deploy-preview-$TRAVIS_PULL_REQUEST--arnellebalane.netlify.com --performance=95 --pwa=100 --accessibility=100 --best-practices=100"
  },
  "repository": "git@github.com:arnellebalane/arnellebalane.com.git",
  "author": "Arnelle Balane <arnellebalane@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "0.18.0"
  },
  "devDependencies": {
    "@babel/preset-env": "7.1.6",
    "@babel/register": "7.0.0",
    "css-loader": "1.0.1",
    "eslint": "5.9.0",
    "eslint-config-arnellebalane": "3.3.0",
    "eslint-plugin-unicorn": "6.0.1",
    "file-loader": "2.0.0",
    "html-critical-webpack-plugin": "2.1.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.2.0",
    "lighthouse-ci": "1.3.0",
    "lint-staged": "8.1.0",
    "mini-css-extract-plugin": "0.4.5",
    "netlify-lambda": "1.0.3",
    "npm-run-all": "4.1.5",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "preload-webpack-plugin": "3.0.0-beta.3",
    "stylelint": "9.8.0",
    "stylelint-config-arnellebalane": "1.1.0",
    "webpack": "4.26.1",
    "webpack-cli": "3.2.0",
    "webpack-dev-server": "3.1.10",
    "webpack-pwa-manifest": "3.7.1",
    "workbox-webpack-plugin": "3.6.3"
  },
  "engines": {
    "node": ">= 8.11.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "eslintConfig": {
    "extends": [
      "arnellebalane",
      "arnellebalane/rules/plugins/unicorn"
    ],
    "plugins": [
      "unicorn"
    ],
    "parserOptions": {
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "node": true
    },
    "globals": {
      "AmbientLightSensor": true
    }
  },
  "stylelint": {
    "extends": [
      "stylelint-config-arnellebalane"
    ],
    "rules": {
      "no-descending-specificity": null
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "npm run lint:js",
    "*.css": "npm run lint:css"
  }
}
